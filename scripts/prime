#! /usr/local/bin/slope

function is_prime(x){
	if((x : 1) + (x : 2)){
		return(1)
	}
	i = 2
	while(i < x){
		if(x%i : 0){
			return(0)
		 }
		i = i + 1
	}
	return(1)
}


function main(){
	i = 1
	n = 0
	print("Last number?")
	input(n)
	while(i < n){
		if(is_prime(i) : 1){
			print(i)
		}
		i = i + 1
	}
}
